{
  "name": "winston-cloudwatch",
  "version": "6.3.0",
  "description": "Send logs to Amazon Cloudwatch using Winston.",
  "keywords": [
    "amazon",
    "cloudwatch",
    "winston",
    "logging",
    "log"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepack": "npm run build",
    "docker:build": "docker build -t winston-cloudwatch-build .",
    "docker:start": "docker run -v `pwd`:/workspace --rm -it winston-cloudwatch-build /bin/sh",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": "https://github.com/ambi-robotics/winston-cloudwatch",
  "author": "me@lazywithclass.com",
  "license": "MIT",
  "peerDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^^3.890.0",
    "winston": "^3.17.0"
  },
  "dependencies": {
    "fast-safe-stringify": "^2.1.1",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.891.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@jest/globals": "^30.1.2",
    "@types/jest": "^30.0.0",
    "@types/node": "22.15.19",
    "@types/winston": "2.4.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "knip": "^5.63.1",
    "ts-jest": "^29.4.3",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=22.17.0"
  }
}